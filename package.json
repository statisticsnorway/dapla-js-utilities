{
  "name": "@statisticsnorway/dapla-js-utilities",
  "version": "0.1.1",
  "description": "JavaScript component library for Statistics Norway Dataplatform applications",
  "main": "lib/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/statisticsnorway/dapla-js-utilities"
  },
  "author": "Statistics Norway",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "rollup --config",
    "test": "jest test",
    "coverage": "jest test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.17",
    "babel-jest": "^26.1.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "jest": "^26.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rollup": "^2.23.0",
    "rollup-plugin-terser": "^7.0.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^1.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/main.js",
      "!src/**/index.js",
      "!src/configurations/**",
      "!src/enums/**",
      "!src/utilities/CustomMethods.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    }
  },
  "directories": {
    "lib": "lib"
  },
  "contributors": [
    {
      "name": "Magnus M. Jenssen"
    },
    {
      "name": "Fenglin Han"
    }
  ]
}
