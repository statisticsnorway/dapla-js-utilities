{
  "name": "@statisticsnorway/dapla-js-utilities",
  "version": "0.2.7",
  "description": "JavaScript common components library for Statistics Norway Dataplatform applications",
  "main": "lib/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/statisticsnorway/dapla-js-utilities"
  },
  "author": "Statistics Norway",
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.3",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "package": "rollup --config",
    "test": "react-scripts test",
    "coverage": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.0.17",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "rollup": "^2.23.0",
    "rollup-plugin-terser": "^7.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**",
      "!src/main.js",
      "!src/**/index.js",
      "!src/components/ssb-logo-*.svg",
      "!src/components/ssb-logo-no-text-*.svg",
      "!src/configurations/**",
      "!src/enums/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 75,
        "functions": 75,
        "lines": 75,
        "statements": 75
      }
    },
    "coverageReporters": [
      "cobertura",
      "lcov",
      "text"
    ]
  },
  "directories": {
    "lib": "lib"
  },
  "contributors": [
    {
      "name": "Magnus M. Jenssen"
    },
    {
      "name": "Fenglin Han"
    }
  ]
}
